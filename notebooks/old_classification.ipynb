    "def calculate_class_weights(Y):\n",
    "    labels = Y.unique()\n",
    "    labels.sort()\n",
    "    a = Y.mode()[0]  # самый часто встреающийся элемент\n",
    "    class_weights = dict((label, (Y == a).sum() / (Y == label).sum()) for label in labels)\n",
    "\n",
    "    return class_weights"
   ]
  },
  {
   "cell_type": "code",
